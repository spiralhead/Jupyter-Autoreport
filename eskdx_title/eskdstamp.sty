\newcommand{\ESKDthisPut}[1]{\def\ESKD@extra@ThisHook{#1}}

\newcommand{\ESKDclearDoublePage}{%
  \ifESKD@twoside%
    \clearpage%
    \ifodd\c@page\else\ESKDthisStyle{empty}\rule{0mm}{0mm}\newpage\fi%
  \else\clearpage\fi}

